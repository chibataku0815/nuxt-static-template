{
  "plugins": ["stylelint-prettier", "stylelint-scss"],
  "processors": [],
  "extends": [
    "stylelint-config-rational-order",
    "stylelint-config-recommended",
    "stylelint-config-standard"
  ],
  "ignoreFiles": [
    "**/node_modules/**"
  ],
  "rules": {
    "prettier/prettier": true,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true
  }
}
